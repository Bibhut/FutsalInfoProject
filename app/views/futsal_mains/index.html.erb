<html>
</<html>
<head>
	
</head>
<body>
	<img src="http://2.bp.blogspot.com/_4AiJIhyVEkM/TSsvL2Sc-dI/AAAAAAAAAJ8/8NimMlOdI4k/s1600/futsal.jpg">
	<div class="mainDiv">
<h3>Futsal List</h3>
<table class='table table-bordered'>
	<th>
		Futsal Name
	</th>
	<th colspan="4">
		Actions
	</th>
	<% @futsal_mains.each do|list|%>
	
	<tr>
		<td>
			<%=list.futsal_name%>
		</td>
		<td>
		<%=link_to 'Edit', edit_futsal_main_path(list.id)%>
		</td>
		<td>
		<%=link_to 'Add Branches',new_futsal_main_futsal_branch_path(list.id)%>
		</td>
		<td>
		<%=link_to 'View Branches', futsal_main_futsal_branches_path(list.id)%>
		</td>
		<td>
		<%= link_to 'Delete',futsal_main_path(list.id), method: 'delete'%>
		</td>
	</tr>
	<%end%>
</table>
<%=link_to 'Add new Futsal', new_futsal_main_path(), {:class => 'btn btn-success'}%>
</div>
</body>
</html>

